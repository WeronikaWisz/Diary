<h1> 
	<%= @entry.title %>
</h1>
  <div><h2><%= @entry.title %></h2>
  <p><%= @entry.date %></p>
  <p><%= @entry.contents %></p>
  <p><%= link_to("Back to all entries", entries_path) %></p>
  <p><%= link_to("Edit this entry", edit_entry_path(@entry)) %></p>
  <p><%= link_to('Delete entry', entry_path(@entry), method: :delete) %></p>
  <p><%= link_to('Image', image_entry_path(@entry)) %></p></div>